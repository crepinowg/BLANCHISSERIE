<div class="row">
    <div class="col-md-12">
        <div class="main-card mb-3 card">
            <div class="card-header">ACTIVITE EN TEMPS REEL DU CLIENT
                <div class="btn-actions-pane-right">
                    <div role="group" class="btn-group-sm btn-group">
                        <button class="btn btn-focus">All Time</button>
                    </div>
                </div>
            </div>
            <div class="table-responsive">
                <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                    <thead>
                        <tr>
                            <th class="text-center">#</th>
                            <th>N° Facture</th>
                            <th class="text-center"></th>
                            <th class="text-center">Total TVA</th>
                            <th class="text-center">Total TTC</th>
                            <th class="text-center">Taux de réduction</th>
                            <th class="text-center">Statut</th>
                            <th class="text-center">Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for factures in facture %}
                        <tr>
                            <td class="text-center text-muted">{{loop.index}}</td>
                            <td>
                                <div class="widget-content p-0">
                                    <div class="widget-content-wrapper">
                                        <div class="widget-content-left mr-3">
                                            <div class="widget-content-left">
                                                <img width="40" class="rounded-circle" src=""
                                                    alt="">
                                            </div>
                                        </div>
                                        <div class="widget-content-left flex2">
                                            <div class="widget-heading">#{{factures.factureidnumber}}</div>
                                            <div class="widget-subheading opacity-7">Deli: {{factures.datelivraison | date('d M Y')}}</div>
                                        </div>
                                    </div>
                                </div>
                            </td>
                            {% if factures.livraison.express == 1 %}
                            <td class="text-center">
                                <div class="badge badge-primary">EXPRESS</div>
                            </td>
                            {% endif %}
                            {% if factures.livraison.express == 0 %}
                            <td class="text-center">
                                <div class=""></div>
                            </td>
                            {% endif %}
                            <td class="text-center">
                                <div class="badge badge-info">{{factures.totaltva | number_format}}</div>
                            </td>
                            <td class="text-center">
                                <div class="badge badge-warning">{{factures.totalttc | number_format}}</div>
                            </td>
                            <td class="text-center" style="width: 150px;">
                                <div class="badge badge-primary">{{factures.tauxreduction}}%</div>
                            </td>

                            {% if factures.etat=='LAVAGE' %}
                            <td class="text-center" style="width: 150px;">
                                <div class="text-uppercase badge badge-danger">LAVAGE</div>
                            </td>
                            {% elseif factures.etat=='ATTENTE' %}
                            <td class="text-center" style="width: 150px;">
                                <div class="text-uppercase badge badge-warning">ATTENTE DE LIVRAISON</div>
                            </td>
                            {% elseif factures.etat=='LIVRAISON' %}
                            <td class="text-center" style="width: 150px;">
                                <div class="text-uppercase badge badge-success">LIVRER & PAYER</div>
                            </td>
                            {% endif %}
                            <td class="text-center">
                                <a href="{{path('app.facture.show' , {id: factures.id})}}"><button type="button" id="PopoverCustomT-1"
                                    class="btn btn-primary btn-sm">Details</button></a>
                            </td>
                        </tr>

                        {% endfor %}
                    </tbody>
                </table>
            </div>
            <div class="d-block text-center card-footer">
                
            </div>
        </div>
    </div>
